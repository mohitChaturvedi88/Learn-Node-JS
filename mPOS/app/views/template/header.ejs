<header class="main-header">
  <!-- Logo -->
  <a href="" class="logo">
    <!-- mini logo for sidebar mini 50x50 pixels -->
    <span class="logo-mini"><b>P</b>OS</span>
    <!-- logo for regular state and mobile devices -->
    <span class="logo-lg"><b>Modern</b>POS</span>
  </a>
  <!-- Header Navbar: style can be found in header.less -->
  <nav class="navbar navbar-static-top">
    <!-- Sidebar toggle button-->
    <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </a>

    <div class="navbar-custom-menu">
      <ul class="nav navbar-nav">
        <!-- Messages: style can be found in dropdown.less-->
        <li>
    <li class="hidden-xs hidden-sm"><a href="#" class="clock"><% include clock.ejs%></a></li>
    <li class="hidden-xs"><a href="/welcome" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Dashboard"><i class="fa fa-dashboard"></i></a></li>
   
     <li><a href="/mPOS" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="POS"><i class="fa fa-th"></i></a></li>
    
     <li><a href="/open_sale" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Hold Bills"><i class="fa fa-pause"></i></a></li>
    
    
          <ul class="dropdown-menu">
            <li class="header">You have 4 messages</li>
            <li>
              <!-- inner menu: contains the actual data -->
              <ul class="menu">
                <li><!-- start message -->
                  <a href="#">
                    <div class="pull-left">
                      <img src="<%= avatar %>" class="img-circle" alt="User Image">
                    </div>
                    <h4>
                      Support Team
                      <small><i class="fa fa-clock-o"></i> 5 mins</small>
                    </h4>
                    <p>Why not buy a new awesome theme?</p>
                  </a>
                </li>
                <!-- end message -->
              </ul>
            </li>
            <li class="footer"><a href="#">See All Messages</a></li>
          </ul>
        </li>
        <!-- Notifications: style can be found in dropdown.less -->
        
        <!-- Tasks: style can be found in dropdown.less -->
       
        <!-- User Account: style can be found in dropdown.less -->
        <li class="dropdown user user-menu">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">
            <img src="<%= avatar %>" class="user-image" alt="User Image">
            <span class="hidden-xs">  <%= fname %> <%= lname %></span>
          </a>
          <ul class="dropdown-menu">
            <!-- User image -->
            <li class="user-header">
              <img src="<%= avatar %>" class="img-circle" alt="User Image">

              <p>
                <%= fname %> <%= lname %>
                <small>Member since : <%= joindates %></small>
              </p>
            </li>
           
            <!-- Menu Footer-->
            <li class="user-footer">
              <div class="pull-left">
                <a href="#" class="btn btn-default btn-flat">Profile</a>
              </div>
              <div class="pull-right">
                <a href="http://localhost:3000/api/logout" name = "signout" class="btn btn-default btn-flat">Sign out</a>
                <script>
                  $('signout').click(function(){
                      $.post('/api/logout')
                  });
              </script>
              </div>
            </li>
          </ul>
        </li>
        <!-- Control Sidebar Toggle Button -->
        
      </ul>
    </div>
  </nav>

<Style>
.divider {
  border-top: 1px solid rgba(127,127,127,.1);
  -webkit-background-clip: padding-box;
  background-clip: padding-box;
  margin: 4px 0 4px -5px!important;
  display: block;
}
</Style>

</header>